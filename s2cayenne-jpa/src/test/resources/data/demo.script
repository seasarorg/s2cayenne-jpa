CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE SEQUENCE MYSEQ AS INTEGER START WITH 32 
CREATE MEMORY TABLE EMP(EMPNO INTEGER NOT NULL PRIMARY KEY,ENAME VARCHAR(10),JOB VARCHAR(9),MGR NUMERIC(4),HIREDATE DATE,SAL NUMERIC(7,2),COMM NUMERIC(7,2),DEPTNO NUMERIC(2),CITY VARCHAR(10),ZIP VARCHAR(10),TSTAMP TIMESTAMP)
CREATE MEMORY TABLE DEPT(DEPTNO INTEGER NOT NULL PRIMARY KEY,DNAME VARCHAR(14),LOC VARCHAR(13),VERSIONNO INTEGER,ACTIVE BOOLEAN)
CREATE MEMORY TABLE EMP2(EMPNO INTEGER NOT NULL PRIMARY KEY,ENAME VARCHAR(10),DEPTNUM INTEGER)
CREATE MEMORY TABLE DEPT2(DEPTNO INTEGER NOT NULL PRIMARY KEY,DNAME VARCHAR(14),ACTIVE BOOLEAN)
CREATE MEMORY TABLE DUMMY(ID INTEGER NOT NULL PRIMARY KEY)
CREATE MEMORY TABLE DUMMY2(ID INTEGER NOT NULL PRIMARY KEY)
CREATE MEMORY TABLE IDENTITYTABLE(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,NAME VARCHAR(20))
CREATE MEMORY TABLE DUAL(DUMMY VARCHAR(1))
CREATE MEMORY TABLE SEQTABLE(ID INTEGER NOT NULL PRIMARY KEY,NAME VARCHAR(20))
ALTER TABLE IDENTITYTABLE ALTER COLUMN ID RESTART WITH 0
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 60
SET SCHEMA PUBLIC
